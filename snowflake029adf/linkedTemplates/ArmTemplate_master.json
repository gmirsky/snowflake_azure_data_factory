{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "snowflake029adf"
		},
		"ls_snowflake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_snowflake'"
		},
		"ls_testdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testdata029asa.dfs.core.windows.net/"
		},
		"tr_on_arrival_of_new_machines_csv_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d3b7bcaf-014c-4a5e-95fe-af002a14ab06/resourceGroups/snowflake_datafactory_rg/providers/Microsoft.Storage/storageAccounts/testdata029asa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "snowflake029adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_snowflake_connectionString": {
						"value": "[parameters('ls_snowflake_connectionString')]"
					},
					"ls_testdata_properties_typeProperties_url": {
						"value": "[parameters('ls_testdata_properties_typeProperties_url')]"
					},
					"tr_on_arrival_of_new_machines_csv_properties_typeProperties_scope": {
						"value": "[parameters('tr_on_arrival_of_new_machines_csv_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}